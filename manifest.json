{
    "name": "VandyHacks Holiday Workshop",
    "description": "Learn how to build a Chrome extension!",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
    "background": {
        "service_worker": "./background.js"
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"]
    
}