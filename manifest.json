{
    "name": "VandyHacks Fall Workshop",
    "description": "Learn how to build a Chrome extension!",
    "version": "0.1.0",
    "manifest_version": 2,
    "background": {
        "service_worker": "./background.js"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"]
    
}